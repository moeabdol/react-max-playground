{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "jquery": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "plugins": [ "react" ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1,
        "FunctionExpression": {
          "body": 1,
          "parameters": 2
        }
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": 0,
    "keyword-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "react/prop-types": 0
  },
  "globals": {
    "$": true,
    "document": true,
    "window": true,
    "google": true,
    "io": true,
    "moment": true,
    "Handlebars": true
  }
}
